{
  "name": "tinier",
  "version": "0.5.1",
  "description": "Tinier is a library for building tiny reactive components in JavaScript. Designed with D3.js in mind.",
  "main": "lib/tinier.js",
  "files": [
    "lib/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakandrewking/tinier.git"
  },
  "author": "Zachary King",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakandrewking/tinier/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --compilers ts:ts-node/register,tsx:ts-node/register src/test-tinier.tsx || true",
    "test-debug": "./node_modules/.bin/mocha debug --compilers ts:ts-node/register,tsx:ts-node/register src/test-tinier.tsx || true",
    "compile": "./node_modules/.bin/tsc && ./node_modules/.bin/uglifyjs --compress --mangle --source-map lib/tinier.min.js.map --in-source-map lib/tinier.js.map -o lib/tinier.min.js -- lib/tinier.js",
    "compilew": "./node_modules/.bin/tsc --watch"
  },
  "homepage": "https://github.com/zakandrewking/tinier#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/jsdom": "^2.0.30",
    "@types/mocha": "^2.2.39",
    "chai": "^3.4.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "typescript": "^2.1.6",
    "uglify-js": "^2.7.3"
  }
}
